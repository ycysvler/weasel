schema {
        query: Query
		mutation: Mutation
}
# 查询
type Query {
        hello: String!
		lala : String!
		user(name:String!, old:Int!) : String!
        group: Group!
}
# 修改
type Mutation {
        addGroup(
            name : String!
			user : UserInput!
        ) : Boolean!
}


input UserInput{
        name : String!
        old  : Int!
}